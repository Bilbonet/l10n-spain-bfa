        <field name="alignment">right</field>
    </record>
    <!--          238-240     Alfanumérico    Nº cuenta PGC-->
    <record id="bfa_mod140_incomes_tax_export_line_28" model="aeat.model.export.config.line">
        <field name="sequence">28</field>
        <field name="export_config_id" ref="bfa_mod140_incomes_tax_export_config"/>
        <field name="name">Nº cuenta PGC</field>
        <field name="expression">${object.account_pgc}</field>
        <field name="export_type">string</field>
        <field name="size">3</field>
        <field name="alignment">left</field>
    </record>
    <!--          241         Alfanumérico    Signo importe ingreso-->
    <!--          242-254     Numérico        Importe ingreso-->
    <record id="bfa_mod140_incomes_tax_export_line_29" model="aeat.model.export.config.line">
        <field name="sequence">29</field>
        <field name="export_config_id" ref="bfa_mod140_incomes_tax_export_config"/>
        <field name="name">Signo / Importe ingreso</field>
        <field name="expression">${object.base_amount}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True"/>
        <field name="positive_sign"> </field>
